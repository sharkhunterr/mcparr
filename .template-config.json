{
  "templateRepo": "https://gitlab.com/your-org/gitlab-project-template.git",
  "templateVersion": "1.0.0",
  "features": {
    "backend": {
      "enabled": true,
      "testCommand": "npm run test:backend"
    },
    "frontend": {
      "enabled": true,
      "testCommand": "npm run test:frontend",
      "buildCommand": "npm run build"
    },
    "docker": {
      "enabled": true,
      "registry": "docker.io"
    },
    "githubMirror": {
      "enabled": true,
      "repository": "sharkhunterr"
    }
  },
  "managedFiles": [
    "scripts/update-deps.js",
    "scripts/sync-template.js",
    "scripts/docker-test.js",
    ".gitlab/ci/*.yml"
  ],
  "protectedFiles": [
    "package.json",
    "Dockerfile",
    ".gitlab-ci.yml",
    "README.md",
    "src/**/*"
  ]
}